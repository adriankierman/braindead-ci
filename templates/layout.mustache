<!DOCTYPE html>
<!--[if IE]>         <html class="ie" lang="en"> <![endif]-->
<!--[if !(IE)]><!--> <html lang="en"> <!--<![endif]-->
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{{title}}{{^title}}Cranium CI{{/title}}</title>
    <meta name="description" content="">
    <meta name="author" content="">

    <link rel="stylesheet" href="/assets/bootstrap/css/bootstrap.css">
    <link rel="stylesheet" href="/assets/css/website.css">
  </head>

  <body>
    <script src="/assets/jquery/jquery-1.8.3.min.js" type="text/javascript" charset="utf-8"></script>
    <script src="/assets/bootstrap/js/bootstrap.js" type="text/javascript" charset="utf-8"></script>

    <div class="container">
      <div class="row">
        <div class="span3">
          <h1 class="top-left-title"><a href="/">Cranium CI</a></h1>
          {{#loggedUser}}
          {{#settingsPage}}
            {{>settingsMenu}}
          {{/settingsPage}}

          {{^settingsPage}}
            {{>normalMenu}}
          {{/settingsPage}}
          {{/loggedUser}}

          <hr>
          <strong>Executor status:</strong><br>
          {{#idle}}
            Cranium is idle
          {{/idle}}
          {{^idle}}
            Building {{currentProjectName}}
          {{/idle}}
          <br><br>
          {{#someProjectsQueued}}
            In the queue:
            <ul>
              {{#queuedProjects}}
                <li>{{.}}</li>
              {{/queuedProjects}}
            </ul>
          {{/someProjectsQueued}}

        </div>

        <div class="span9">
          {{>content}}
        </div>
      </div>

    </div>

  </body>
</html>


